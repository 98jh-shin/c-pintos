{
    "name": "pintos-dev",
    "dockerFile": "Dockerfile",
    "remoteUser": "jungle",
    "mounts": [
        "source=${localWorkspaceFolder}/pintos,target=/home/jungle,type=bind"
    ],
    "workspaceFolder": "/home/jungle/pintos",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-vscode.cpptools-extension-pack",
                "ms-vscode.cpptools",
                "ms-azuretools.vscode-docker",
                "ms-vscode.makefile-tools"
            ],
            "settings": {
                "terminal.integrated.defaultProfile.linux": "bash"
            }
        }
    }
}